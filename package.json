{
  "name": "whistle.inspect",
  "version": "2.3.0",
  "description": "集成 vConsole、eruda、mdebug 等调试H5页面工具的插件",
  "scripts": {
    "build": "node ./src/build"
  },
  "homepage": "https://github.com/whistle-plugins/whistle.inspect",
  "keywords": [
    "whistle",
    "eruda",
    "vConsole",
    "inspector",
    "h5"
  ],
  "whistleConfig": {
    "hintList": [
      "vConsole",
      "eruda",
      "mdebug"
    ],
    "hideShortProtocol": true
  },
  "registry": "https://github.com/whistle-plugins/whistle.inspect",
  "repository": {
    "type": "git",
    "url": "https://github.com/whistle-plugins/whistle.inspect.git"
  },
  "engines": {
    "node": ">= 6"
  }
}
